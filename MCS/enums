RESERVATION_STATUS
	'1' = New Reservation
	'2' = Denied Reservation
	'3' = Accepted Reservation

TRANSACTION_STATUS
	'1' = Pending Transaction
	'2' = Fully Paid
	'3'
	'4'
	'5' = Needs to Pay additional amount due to loss equipment
	'6' = Denied Reservation

PAYMENT_TERM
	'1' = Full Payment
	'2' = Half Payment
	'3' = 70/30 Payment

PAYMENT_STATUS
	'0' = Pending
	'1' = Paid